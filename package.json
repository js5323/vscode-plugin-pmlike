{
  "name": "vscode-plugin-pmlike",
  "displayName": "vscode-plugin-pmlike",
  "engines": {
    "vscode": "^1.99.0"
  },
  "publisher": "js5323",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "main": "dist/extension/index.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vscode-plugin-pmlike.showPMLike",
