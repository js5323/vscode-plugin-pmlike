    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tomjs/tsconfig": "^1.7.2",
    "@tomjs/vite-plugin-vscode": "^4.0.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/vscode": "^1.100.0",
    "@types/vscode-webview": "^1.57.5",
